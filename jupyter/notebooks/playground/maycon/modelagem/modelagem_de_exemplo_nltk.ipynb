    "## para cada linha, envie uma requisição post\n",
    "import requests\n",
    "import json\n",
    "\n",
    "def enviar_requisicao_post(row):\n",
    "    url = 'http://20.241.232.187/news'\n",
    "    headers = {'Content-type': 'application/json'}\n",
    "    # parse string to datetime\n",
    "    date_publication = \"2023-05-06T18:11:21.749Z\"\n",
    "    data = {\n",
    "  \"title\": row['titulo'],\n",
    "  \"newsContent\": row['noticia'],\n",
    "  \"publicationDate\": date_publication,\n",
    "  \"commodityType\": \"Soja\",\n",
    "  \"source\": \"CEPEA\",\n",
    "  \"positiveSentiment\": row['Positiva'],\n",
    "  \"neutralSentiment\": row['Neutra'],\n",
    "  \"negativeSentiment\": row['Negativa'],\n",
    "  \"finalSentiment\": row['predict_final'],\n",
    "        }\n",
    "\n",
    "    response = requests.post(url, data=json.dumps(data), headers=headers)\n",
    "    print(response.status_code)\n",
    "\n",
    "df_pred_concatenado.apply(enviar_requisicao_post, axis=1)\n"
   ]
